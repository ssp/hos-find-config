<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>
			Language labels for facet content.
			The file »locallang-facets.xml« will be loaded from the path set as
			»settings.languageRootPath« in TypoScript.

			Localisation keys / label indexes follow the convention:
				* facet name for index FOO: facet.FOO
				* facet name for value BAR in index FOO: facet.FOO.BAR
		</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="facet.source">Sammlung</label>
			<label index="facet.source.ediss">E-Dissertationen Uni Hamburg</label>
			<label index="facet.source.hsu">Publikationen HSU</label>
			<label index="facet.source.tuhh-fdm">Forschungsdaten TU Hamburg</label>

			<label index="facet.openaccess">OpenAccess</label>
			<label index="facet.year">Erscheinungsjahr</label>
			<label index="facet.map">Karte</label>
			<label index="facet.publisher">HerausgeberIn / Verlag</label>
			<label index="facet.subject">Schlagworte</label>
			<label index="facet.ddc">Fachgebiete</label>
			<label index="facet.creator">AutorIn</label>
			<label index="facet.type">Ressourcentyp</label>
			<label index="facet.language">Sprache</label>
		</languageKey>
	</data>
</T3locallang>